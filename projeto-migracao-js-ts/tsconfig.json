{
  "compilerOptions": {
    "target": "ESNext",                               
    "module": "ESNext",
    "moduleResolution": "Bundler",                             
    "esModuleInterop": true,                       
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                     
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "noEmit": true,
    "allowJs": true                           
  }
}
